{
    "build-runtime": true,
    "id": "org.kde.Sdk",
    "id-platform": "org.kde.Platform",
    "branch": "master",
    "runtime": "org.freedesktop.Platform",
    "sdk": "org.freedesktop.Sdk",
    "initial-debug": "org.freedesktop.Debug",
    "runtime-version": "master",
    "writable-sdk": true,
    "sdk-extensions": ["org.freedesktop.Sdk.Debug"],
    "platform-extensions": [],
    "metadata": "metadata.sdk",
    "metadata-platform": "metadata.platform",
    "cleanup": [ "/cache",
                 "/man",
                 "/share/man",
                 "/share/gtk-doc",
                 "/share/devhelp",
                 "/lib/systemd",
                 "*.la", "*.a"],
    "cleanup-commands": [ "touch -d @0 /usr/share/fonts",
                          "touch -d @0 /usr/share/fonts/*",
                          "fc-cache -fs"
                        ],
    "cleanup-platform": [ "/include",
                          "/share/aclocal",
                          "/share/pkgconfig",
                          "/lib/pkgconfig",
                          "/share/gir-1.0",
                          "/share/vala"
                        ],
