                {
                    "type": "archive",
                    "url": "http://download.qt.io/archive/qt/5.9/5.9.7/submodules/qtbase-opensource-src-5.9.7.tar.xz",
                    "sha256": "36dd9574f006eaa1e5af780e4b33d11fe39d09fd7c12f3b9d83294174bd28f00"
                },
                {
                    "type": "shell",
                    "commands": [ "mv configure configure.qt" ]
                },
                {
                    "type": "patch",
                    "path": "patch/qtbase-flatpak-portals-filechooser.patch"
                },
                {
                    "type": "patch",
                    "path": "patch/qtbase-flatpak-portals-services.patch"
                },
                {
                    "type": "patch",
