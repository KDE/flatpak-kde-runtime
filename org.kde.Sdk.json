                             "-platform", "linux-g++",
                             "-optimized-qmake",
                             "-verbose",
                             "-nomake", "examples",
                             "-system-harfbuzz",
                             "-system-sqlite",
                             "-no-alsa",
                             "-accessibility",
                             "-dbus-linked",
                             "-fontconfig",
                             "-glib",
                             "-iconv",
                             "-icu",
                             "-openssl-linked",
                             "-no-pch",
                             "-no-rpath",
                             "-no-separate-debug-info",
                             "-reduce-relocations",
                             "-no-directfb",
                             "-no-linuxfb",
                             "-no-kms",
                             "-no-cups",
                             "-system-proxies"
                           ],
