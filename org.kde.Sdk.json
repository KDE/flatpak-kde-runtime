                    "type": "patch",
                    "path": "patch/qtbase-avoid-hardcoding-kernel-version.patch"
                },
                {
                    "type": "patch",
                    "path": "patch/qtbase-revert-reset-qwidget-winid-when-backing-window-surface-destroyed.patch"
                },
                {
                    "type": "shell",
                    "commands": [ "mv configure configure.qt" ]
                },
                {
                    "type": "script",
                    "commands": [
                        "processed=`sed -e 's/--/-/g ; s/=/ /g' <<< $@`",
                        "./configure.qt $processed"
