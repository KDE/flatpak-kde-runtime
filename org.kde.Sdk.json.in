            "name": "qt5-qtbase",
            "cleanup-platform": [
                "/bin"
            ],
            "build-options": {
                "arch": {
                    "x86_64": {
                        "config-opts": [
                            "-reduce-relocations",
                            "-force-debug-info"
                        ]
