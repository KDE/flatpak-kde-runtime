            "config-opts": [
                "-confirm-license",
                "-opensource",
                "-shared",
                "-platform",
                "linux-g++",
                "-optimized-qmake",
                "-nomake",
                "examples",
                "-nomake",
                "tests",
                "-system-harfbuzz",
                "-system-sqlite",
                "-accessibility",
                "-dbus-linked",
                "-fontconfig",
                "-glib",
                "-icu",
                "-openssl-linked",
                "-no-pch",
                "-no-rpath",
