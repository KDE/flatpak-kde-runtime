                             "-system-harfbuzz",
                             "-system-sqlite",
                             "-accessibility",
                             "-dbus-linked",
                             "-fontconfig",
                             "-glib",
                             "-icu",
                             "-openssl-linked",
                             "-no-pch",
                             "-no-rpath",
                             "-no-directfb",
